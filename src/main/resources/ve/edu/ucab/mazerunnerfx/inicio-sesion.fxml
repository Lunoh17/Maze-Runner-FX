<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="402.0" prefWidth="635.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ve.edu.ucab.mazerunnerfx.InicioSesionController">
   <children>
      <ImageView fitHeight="409.0" fitWidth="635.0" layoutY="-2.0" pickOnBounds="true">
         <image>
            <Image url="@../../../../misc/inicioSesionReal.jpg" />
         </image>
      </ImageView>
      <AnchorPane layoutX="14.0" layoutY="62.0" prefHeight="284.0" prefWidth="303.0">
         <children>
            <ImageView fitHeight="283.0" fitWidth="303.0" opacity="0.65" pickOnBounds="true">
               <image>
                  <Image url="@../../../../misc/whiteBlock.png" />
               </image>
            </ImageView>
            <Label layoutX="14.0" layoutY="27.0" text="Correo">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="email" layoutX="14.0" layoutY="64.0" prefHeight="30.0" prefWidth="236.0" promptText="ej. name@mail.com">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <!-- inline email error -->
            <Label fx:id="emailError" layoutX="14.0" layoutY="96.0" prefHeight="16.0" prefWidth="236.0" style="-fx-text-fill: red; -fx-font-size: 11px;" text="" />
            <Label layoutX="14.0" layoutY="110.0" text="Contraseña">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <PasswordField fx:id="password" layoutX="14.0" layoutY="137.0" prefHeight="25.0" prefWidth="190.0">
               <font>
                  <Font size="14.0" />
               </font>
            </PasswordField>
            <!-- inline password error -->
            <Label fx:id="passwordError" layoutX="14.0" layoutY="167.0" prefHeight="16.0" prefWidth="236.0" style="-fx-text-fill: red; -fx-font-size: 11px;" text="" />
            <Button fx:id="inicioSesionMaze" layoutX="17.0" layoutY="196.0" mnemonicParsing="false" onAction="#onInicioSesionMaze" text="Iniciar Sesión">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="regresoBoton" layoutX="166.0" layoutY="196.0" mnemonicParsing="false" onAction="#onRegresoBoton" text="Regresar">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
